<header>
	<div class="sponsor round">
	<%= link_to sponsor_logo, root_path, :class => "round" %>
	</div>
	<nav class="round">
		<ul>
			<% if signed_in? %>
			<li><%= link_to "Home", contests_path %></li> 
			<li><%= link_to "Profile", user_path(current_user) %></li> 
			<%end%>
			<% if signed_in? %>
				<% if admin? %>
				<li><%= link_to "Admin", admin_path %></li>
				<li><%= link_to "Users", users_path %></li> 
				<li><%= link_to "Settings", edit_user_path(current_user) %></li> 
				<%end%>
			<li><%= link_to "Sign out", signout_path, :confirm => "You sure?"%></li>
			<% else %>
			<li><%= link_to "Sign in", signin_path %></li>
		</ul>
		<%end%>
	</nav>	
</header>