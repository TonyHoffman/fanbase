<div data-role="content" <%=data_theme%>>
<h1>Sign in Mobile</h1>

<%= form_for(:session, :url => sessions_path) do |f| %>

<div>
		<%= f.label :email %> <br />
		<%= f.text_field :email%>
</div>
<div>
		<%= f.label :password %> <br />
		<%= f.password_field :password%>
</div>
<div>
	<%= f.submit 'Sign in', "data-ajax" => "false"%> <br />
</div>
<%end%>
<p>New User? <%= link_to "Sign up now!", signup_path%></p> 

</div>
